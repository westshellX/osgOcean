#ifndef OSGSKY_VERSION
#define OSGSKY_VERSION

#include <Export>

extern "C" {

#define OSGSKY_VERSION_MAJOR 1
#define OSGSKY_VERSION_MINOR 0
#define OSGSKY_VERSION_PATCH 1

#define OSGSKY_VERSION_RELEASE OSGSKY_VERSION_PATCH
#define OSGSKY_VERSION_REVISION 0

extern OSGSKY_EXPORT const char* OSGSKYGetVersion();

extern OSGSKY_EXPORT const char* OSGSKYGetLibraryName();

}

#endif
